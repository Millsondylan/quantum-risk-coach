<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="false">
        <domain includeSubdomains="true">api.tradingeconomics.com</domain>
        <domain includeSubdomains="true">api.finnhub.io</domain>
        <domain includeSubdomains="true">api.polygon.io</domain>
        <domain includeSubdomains="true">api.alphavantage.co</domain>
        <domain includeSubdomains="true">api.twelvedata.com</domain>
        <domain includeSubdomains="true">api.supabase.co</domain>
    </domain-config>
    <base-config cleartextTrafficPermitted="false">
        <trust-anchors>
            <certificates src="system"/>
        </trust-anchors>
    </base-config>
</network-security-config> 