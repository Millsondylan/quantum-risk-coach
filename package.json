{
  "name": "tradenote-mobile",
  "version": "1.0.0",
  "description": "Mobile-first trading journal with AI analytics and holographic interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "capacitor:add": "npx cap add android",
    "capacitor:sync": "npx cap sync",
    "capacitor:build": "npm run build && npx cap sync",
    "capacitor:open": "npx cap open android",
    "capacitor:run": "npx cap run android",
    "android:build": "npm run capacitor:build && cd android && ./gradlew assembleDebug",
    "android:release": "npm run capacitor:build && cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^7.0.1",
    "@capacitor/android": "^7.4.0",
    "@capacitor/app": "^7.0.0",
    "@capacitor/camera": "^7.0.0",
    "@capacitor/cli": "^7.4.1",
    "@capacitor/core": "^7.4.0",
    "@capacitor/device": "^7.0.0",
    "@capacitor/filesystem": "^7.0.0",
    "@capacitor/local-notifications": "^7.0.1",
    "@capacitor/network": "^7.0.0",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/push-notifications": "^7.0.0",
    "@capacitor/splash-screen": "^7.0.0",
    "@capacitor/status-bar": "^7.0.0",
    "@capacitor/toast": "^7.0.1",
    "@google/generative-ai": "^0.2.1",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.50.3",
    "@vueuse/core": "^10.7.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "axios-retry": "^4.5.0",
    "chart.js": "^4.4.0",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^3.6.0",
    "groq-sdk": "^0.3.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.525.0",
    "openai": "^4.20.1",
    "pinia": "^2.1.7",
    "postcss": "^8.4.32",
    "react": "^19.1.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "recharts": "^3.0.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.0",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "vue": "^3.4.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-carousel": "^0.16.0",
    "vue3-lottie": "^2.0.0",
    "vue3-touch-events": "^4.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/test-utils": "^2.4.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
